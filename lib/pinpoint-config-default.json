{
  "application-type": 1400,
  "collector": {
    "ip": "localhost",
    "span-port": 9993,
    "stat-port": 9992,
    "tcp-port": 9991
  },
  "sampling": {
    "enable": true,
    "rate": 10
  },
  "http-status-code": {
    "errors": "5xx,401,403"
  },
  "logger-levels": {
    "default-logger": "WARN",
    "grpcLogger": "SILENT"
  },
  "enable": true,
  "enabled-data-sending": true,
  "enabled-stats-monitor-sending": true,
  "enabled-active-thread-count": false,
  "container": false,
  "express": {
    "enable": true
  },
  "koa": {
    "enable": true
  },
  "mongo": {
    "enable": true
  },
  "redis": {
    "enable": true
  },
  "stream-deadline-minutes": {
    "client-side": 10
  },
  "trace-location-and-filename-of-call-site": false
}
{
    "applicationServiceType": 1400,
    "collector": {
        "ip": "localhost",
        "spanPort": 9993,
        "statPort": 9992,
        "tcpPort": 9991,
        "deadlineMinutes": 10
    },
    "sampling": {
        "enable": true,
        "rate": 10
    },
    "enable": true,
    "plugins": {
        "http": {
            "errorStatusCodes": "5xx,401,403"
        },
        "express": true,
        "koa": true,
        "mongodb": true,
        "redis": true,
        "ioredis": true,
        "mysql": true,
        "postgresql": true
    },
    "features": {
        "activeThreadCount": true,
        "dataSending": true,
        "statsMonitoring": true,
        "container": false,
        "sqlStats": false,
        "logLevels": {
            "default-logger": "WARN",
            "grpcLogger": "SILENT"
        },
        "traceExclusionUrl": {
            "cacheSize": 100
        }
    }
}